<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <location xdt:Transform="Replace" path="." inheritInChildApplications="false">
	  <system.webServer>
		  <handlers>
			  <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
		  </handlers>
		  <aspNetCore processPath="dotnet" arguments=".\PinoyDevs.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
	  </system.webServer>
  </location>

</configuration>